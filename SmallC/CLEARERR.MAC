 INCLUDE PROLOG.H
 EXTRN _Ustatus:WORD
_CC1:
 PUBLIC _clearerr
_clearerr:
 MOV BX,2
 ADD BX,SP
 MOV BX,[BX]
 PUSH BX  ;
 CALL _Umode
 ADD SP,2
 OR BX,BX
 JNZ $+5
 JMP _CC3
 MOV BX,OFFSET _Ustatus
 XCHG DX,BX
 MOV BX,2
 ADD BX,SP
 MOV BX,[BX]
 ADD BX,BX
 ADD BX,DX
 PUSH BX  ;
 MOV BX,[BX]
 XCHG DX,BX
 MOV BX,-9
 AND BX,DX
 POP DX
 MOV SI,DX
 MOV [SI],BX
_CC3:
 RET
 EXTRN _Umode:NEAR
 INCLUDE EPILOG.H
 END
