 INCLUDE PROLOG.H
_CC1:
 PUBLIC _iscntrl
_iscntrl:
 MOV BX,2
 ADD BX,SP
 MOV BX,[BX]
 XCHG DX,BX
 MOV BX,31
 CMP DX,BX
 MOV BX,CX
 JA $+3
 INC BX
 OR BX,BX
 JZ $+5
 JMP _CC3
 MOV BX,2
 ADD BX,SP
 MOV BX,[BX]
 XCHG DX,BX
 MOV BX,127
 CMP DX,BX
 MOV BX,CX
 JNZ $+3
 INC BX
 OR BX,BX
 JZ $+5
 JMP _CC3
 MOV BX,0
 JMP _CC4
_CC3:
 MOV BX,1
_CC4:
 RET
 INCLUDE EPILOG.H
 END
