 INCLUDE PROLOG.H
_CC1:
 PUBLIC _pad
_pad:
_CC3:
 MOV BX,2
 ADD BX,SP
 MOV DX,BX
 MOV BX,[BX]
 DEC BX
 MOV SI,DX
 MOV [SI],BX
 INC BX
 OR BX,BX
 JNZ $+5
 JMP _CC4
 MOV BX,6
 ADD BX,SP
 MOV DX,BX
 MOV BX,[BX]
 INC BX
 MOV SI,DX
 MOV [SI],BX
 DEC BX
 XCHG DX,BX
 MOV BX,4
 ADD BX,SP
 MOV BX,[BX]
 MOV SI,DX
 MOV [SI],BL
 JMP _CC3
_CC4:
 RET
 INCLUDE EPILOG.H
 END
